  <Quest Name="Zone3Stranger1DifficultyAlert">
    <Events>
      <Activate />
    </Events>
    <Conditions>
      <Equal Value1="_$ActionID" Value2="Zone3Stranger1Alert" />
      <Greater Value1="?Fight(?Concat(ZONE_3|Stranger|lvl,_StrangerTriggerLevel)).Difficulty" Value2="1" />
      <Equal Value1="_Zone3Stranger1DifficultyAlert" Value2="1" Not="1" />
    </Conditions>
    <Actions Place="Map">
      <SetVariable Name="Zone3Stranger1DifficultyAlert" Value="1" />
      <Dialog Type="Regular" Title="characterSensei" Image="character_sensei">
        <Line Text="stranger_sensei_difficulty_1" ButtonText="dlgStoryBtnMore" />
